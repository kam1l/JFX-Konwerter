				<Menu
					mnemonicParsing="false"
					text="Plik">
					<items>
						<MenuItem
							mnemonicParsing="false"
							onAction="#closeApp"
							text="Zamknij">
							<accelerator>
								<KeyCodeCombination
									alt="UP"
									code="W"
									control="DOWN"
									meta="UP"
									shift="UP"
									shortcut="UP" />
							</accelerator>
						</MenuItem>
					</items>
				</Menu>
				<Menu
					mnemonicParsing="false"
					text="Edycja">
					<items>
						<MenuItem
							mnemonicParsing="false"
							onAction="#swapUnits"
							text="Zamień jednostki">
							<accelerator>
								<KeyCodeCombination
									alt="UP"
									code="E"
									control="DOWN"
									meta="UP"
									shift="UP"
									shortcut="UP" />
							</accelerator>
						</MenuItem>
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem
							mnemonicParsing="false"
							onAction="#showPreferences"
							text="Preferencje">
							<accelerator>
								<KeyCodeCombination
									alt="UP"
									code="O"
									control="DOWN"
									meta="UP"
									shift="UP"
									shortcut="UP" />
							</accelerator>
						</MenuItem>
					</items>
				</Menu>
				<Menu
					mnemonicParsing="false"
					text="?">
					<items>
						<MenuItem
							mnemonicParsing="false"
							onAction="#runUpdateThread"
							text="Aktualizuj kursy walut">
							<accelerator>
								<KeyCodeCombination
									alt="UP"
									code="A"
									control="DOWN"
									meta="UP"
									shift="UP"
									shortcut="UP" />
							</accelerator>
						</MenuItem>
						<SeparatorMenuItem mnemonicParsing="false" />
						<MenuItem
							mnemonicParsing="false"
							onAction="#showAppInfo"
							text="O programie">
							<accelerator>
								<KeyCodeCombination
									alt="ANY"
									code="F1"
									control="UP"
									meta="UP"
									shift="UP"
									shortcut="UP" />
							</accelerator>
						</MenuItem>
					</items>
				</Menu>
			</menus>
		</MenuBar>
